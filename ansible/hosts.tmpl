azure_vm:
    hosts:
    ${vm_name}:
        ansible_host: ${vm_public_ip}
        ansible_user: ${vm_username}
        ansible_ssh_private_key_file: ${ssh_private_key}
        ansible_python_interpreter: ${python_interpreter}

azure_acr:
    hosts:
    ${acr_name}:
        acr_login_server: ${acr_login_server}
